generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TableFood {
  id    Int    @id @default(autoincrement())
  foods Food[]
}

model Food {
  id          Int       @id @default(autoincrement())
  name        String
  total       Int
  tableFood   TableFood @relation(fields: [tableFoodId], references: [id])
  tableFoodId Int
}
